{
    "name": "lanos/laravel-open-ai-conversations",
    "description": "Adds persistent conversation functionality to the existing Laravel OpenAI Package",
	"keywords": ["laravel", "openai", "conversations", "gpt"],
    "type": "library",
    "require": {
        "openai-php/laravel": "^v0.5.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.1",
        "orchestra/testbench": "^8.8"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Robert Lane",
            "email": "rob@updev.agency"
        }
    ],
    "autoload": {
        "psr-4": {
            "Lanos\\OpenAiConversations\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lanos\\OpenAiConversations\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
              "Lanos\\OpenAiConversations\\OpenAiConversationsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "post-autoload-dump": [
            "@php vendor/bin/testbench package:discover --ansi"
        ]
    }
}
